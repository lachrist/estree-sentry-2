{
  "name": "estree-sentry",
  "version": "0.1.3",
  "main": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.mjs",
    "!lib/**/*.test.mjs"
  ],
  "keywords": [
    "estree",
    "sentry",
    "guard",
    "escmascript",
    "typescript",
    "annotation"
  ],
  "devDependencies": {
    "@types/estree": "^1.0.6",
    "@types/node": "^22.7.4",
    "@types/source-map": "^0.5.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "acorn": "^8.12.1",
    "astring": "^1.9.0",
    "c8": "^10.1.2",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "format": "npx prettier --check 'lib/**/*' 'test/**/*' '*.mjs'",
    "lint": "npx eslint 'lib/**/*' 'test/**/*' '*.mjs'",
    "doc": "npx typedoc && open typedoc/index.html",
    "deploy-page": "sh deploy-page.sh",
    "test": "npx c8 -- node test/index.mjs reg",
    "test:cov": "node test/index.mjs cov",
    "test:reg": "node test/index.mjs reg"
  }
}
